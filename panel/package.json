{
  "name": "web",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "hyperapp": "^1.2.5",
    "parcel": "^1.8.1"
  },
  "scripts": {
    "start": "parcel web/index.html",
    "build": "rm -f dist/* && parcel build web/index.html",
    "post": "rm dist/mainInfo* && cp web/mainInfo.js dist && sed -i -r -e 's/mainInfo.[a-z0-9]+/mainInfo/' dist/index.html"
  }
}
